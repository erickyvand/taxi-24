{
	"name": "taxi-24",
	"version": "1.0.0",
	"description": "TAXI 24",
	"main": "index.js",
	"scripts": {
		"dev-server": "nodemon --exec babel-node src/index.js ",
		"migrate:up": "npx sequelize-cli db:migrate",
		"migrate:drop": "npx sequelize-cli db:migrate:undo:all",
		"seed:up": "npx sequelize-cli db:seed:all",
		"seed:drop": "npx sequelize-cli db:seed:undo:all",
		"pretest": "npm run migrate:drop && npm run migrate:up && npm run seed:up",
		"coverage": "nyc report --report=text-lcov | coveralls",
		"test": "export NODE_ENV=test && nyc --reporter=html --reporter=text mocha --require @babel/register src/tests/**/*.test.js --timeout 100000 --exit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/erickyvand/taxi-24.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/erickyvand/taxi-24/issues"
	},
	"homepage": "https://github.com/erickyvand/taxi-24#readme",
	"dependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/node": "^7.10.5",
		"@babel/preset-env": "^7.11.5",
		"@babel/register": "^7.11.5",
		"@hapi/joi-date": "^2.0.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"joi": "^17.2.1",
		"pg": "^8.3.3",
		"pg-hstore": "^2.3.3",
		"regenerator-runtime": "^0.13.7",
		"sequelize": "^6.3.5"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"coveralls": "^3.1.0",
		"mocha": "^8.1.3",
		"mocha-lcov-reporter": "^1.3.0",
		"nyc": "^15.1.0",
		"sequelize-cli": "^6.2.0"
	}
}
